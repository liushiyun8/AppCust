    <!-- activity样式-->
    <style name="AppTheme.activity" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/button_text_color</item>
        <item name="colorPrimaryDark">@color/button_text_color</item>
